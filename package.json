    "prettier-plugin-svelte": "^3.2.6",
    "ringbufferjs": "^2.0.0",
    "rxjs": "^7.8.1",
    "svelte": "^4.2.18",
    "svelte-check": "^3.8.5",
    "tailwindcss": "^3.4.10",
    "tslib": "^2.6.3",
